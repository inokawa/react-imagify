{
  "name": "react-imagify",
  "version": "0.1.0",
  "description": "A small React component to render HTML as Canvas.",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c",
    "tsc": "tsc -p . --noEmit",
    "tsc:d": "tsc -p . --outDir lib -d --emitDeclarationOnly",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "prepublishOnly": "rimraf lib && npm run build && npm run tsc:d"
  },
  "devDependencies": {
    "@babel/core": "7.14.2",
    "@rollup/plugin-typescript": "8.2.1",
    "@storybook/addon-essentials": "6.2.9",
    "@storybook/react": "6.2.9",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.5",
    "babel-loader": "8.2.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "3.0.2",
    "rollup": "2.48.0",
    "typescript": "4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inokawa/react-imagify.git"
  },
  "keywords": [
    "react",
    "canvas",
    "webgl"
  ],
  "author": "inokawa <stratoooo-taster@yahoo.co.jp> (https://github.com/inokawa/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inokawa/react-imagify/issues"
  },
  "homepage": "https://github.com/inokawa/react-imagify#readme"
}
